{
  "name": "mongodb-download-url",
  "version": "0.0.2",
  "description": "Lookup download URL's for MongoDB versions.",
  "scripts": {
    "check": "mongodb-js-precommit",
    "test": "mocha",
    "ci": "./node_modules/istanbul/lib/cli.js cover _mocha -- -R spec ./test/*.test.js"
  },
  "bin": {
    "mongodb-download-url": "bin/mongodb-download-url.js"
  },
  "check": {
    "ignore": [
      "coverage/{*,**/*}"
    ]
  },
  "precommit": [
    "check",
    "test"
  ],
  "dependencies": {
    "async": "1.4.2",
    "debug": "2.2.0",
    "lodash.defaults": "3.1.2",
    "minimist": "1.2.0",
    "mongodb-version-list": "0.0.3",
    "request": "2.64.0",
    "semver": "5.0.3"
  },
  "devDependencies": {
    "coveralls": "2.11.4",
    "eslint-config-mongodb-js": "0.1.6",
    "istanbul": "0.3.22",
    "mocha": "2.2.5",
    "mongodb-js-precommit": "0.1.4",
    "pre-commit": "1.1.1"
  },
  "keywords": [
    "mongodb",
    "mongodb.js"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/mongodb-js/mongodb-download-url.git"
  },
  "author": "Lucas Hrabovsky <hrabovsky.lucas@gmail.com> (http://imlucas.com)",
  "license": "Apache-2.0",
  "homepage": "http://github.com/mongodb-js/mongodb-download-url"
}
